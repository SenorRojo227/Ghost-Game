[application]

config/name="Ghost Game"
config/version="0.0.1"
run/main_scene="res://Scenes/Levels/MainMenu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://Assets/Ghost1.png"

[autoload]

Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"Booker": "res://Dialogic/Characters/Booker.dch",
"PlayerGhost": "res://Dialogic/Characters/PlayerGhost.dch",
"Tomato": "res://Dialogic/Characters/Tomato.dch"
}
directories/dtl_directory={
"BookerIntro": "res://Dialogic/Timelines/BookerIntro.dtl",
"Start": "res://Dialogic/Timelines/Start.dtl",
"TomatoInteraction1": "res://Dialogic/Timelines/TomatoInteraction1.dtl",
"TomatoInteraction2": "res://Dialogic/Timelines/TomatoInteraction2.dtl",
